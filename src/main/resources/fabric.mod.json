{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${mod_version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [
    "Argon4W",
    "XIAYM-gh"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },
  "license": "${mod_license}",
  "environment": "client",
  "entrypoints": {
    "client": [
      "com.github.argon4w.acceleratedrendering.AcceleratedRenderingModEntry"
    ],
    "modmenu": [
      "com.github.argon4w.acceleratedrendering.compat.modmenu.ModMenuEntry"
    ]
  },
  "accessWidener": "acceleratedrendering.accesswidener",
  "mixins": [
    "acceleratedrendering.compat.iris.mixins.json",
    "acceleratedrendering.core.mixins.json",
    "acceleratedrendering.feature.entities.mixins.json",
    "acceleratedrendering.feature.modelparts.mixins.json",
    "acceleratedrendering.feature.text.mixins.json"
  ],
  "depends": {
    "fabric-resource-loader-v0": "*"
  },
  "suggests": {
    "modmenu": ">=13.0.3",
    "cloth-config": ">=17.0.144",
    "sodium": ">=0.6.13+mc1.21.4",
    "iris": ">=1.8.8+mc1.21.4"
  },
  "custom": {
    "modmenu:clientsideOnly": true
  }
}